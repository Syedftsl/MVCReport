@model IEnumerable<MVCReport.Models.Student>

@{
    ViewBag.Title = "StudentInfo";
}

<h2>Student Info</h2>

@Html.ActionLink("Download  Excel", "Reports", new { ReportType = "Excel"}) |
@Html.ActionLink("Download  PDF", "Reports", new { ReportType = "PDF"}) |
@Html.ActionLink("Download  Word", "Reports", new { ReportType = "Word"}) |
@Html.ActionLink("Download  Image", "Reports", new { ReportType = "Image"}) |
<br/><br /><br />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mobile)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mobile)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
      
    </tr>
}

</table>
